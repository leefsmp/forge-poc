{
  "name": "forge-poc",
  "description": "Forge POC",
  "version": "1.0.0",
  "author": "Philippe Leefsma",
  "keywords": [
    "create-react-app",
    "react",
    "redux"
  ],
  "scripts": {
    "git-build": "cross-env PUBLIC_URL=\"https://leefsmp.github.io/forge-poc/build/\" node scripts/build.js",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
